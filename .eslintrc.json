{
  "parser": "vue-eslint-parser",
  "extends": ["eslint:recommended", "plugin:vue/essential"],
  "parserOptions": {
    "parser": "@babel/eslint-parser",
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "plugins": ["vue"],
  "rules": {
    "no-unused-vars": 0
  }
}
